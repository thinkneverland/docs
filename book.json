{
    "title": "Think Neverland Documentation",
    "description": "Documentation for all Think Neverland packages",
    "language": "en",
    "plugins": [
        "theme-api",
        "search",
        "expandable-chapters",
        "back-to-top-button",
        "copy-code-button",
        "heading-anchors",
        "custom-favicon"
    ],
    "pluginsConfig": {
        "theme-api": {
            "theme": "dark"
        },
        "search": {
            "maxIndexSize": 100000,
            "fuzzy": true
        },
        "custom-favicon": {
            "favicon": "assets/favicon.ico"
        }
    },
    "gitbook": "3.2.3",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "links": {
        "sidebar": {
            "Think Neverland": "https://thinkneverland.com"
        }
    }
}